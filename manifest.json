{
  "name": "Riyusaki",
  "description": "chrome extension for riyusaki.com",
  "version": "2.0",
  "permissions": [
    "tabs",
    "cookies",
    "http://www.riyusaki.com/",
    "http://localhost:5000/log",
    "<all_urls>",
    "storage",
    "webRequest"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2
}
